            }
            .add>p {
                font-size: 3.5rem;
            }
            .text-color>a {
                font-size: 3rem;
            }
        }
        
        @media (min-width: 500px) and (max-width: 799px) {
            html {
                font-size: 25%;
            }
            .box>h1 {
                font-size: 5rem;
            }
            .bod {
                height: 105rem;
            }
            .vid {
                width: 90rem;
                margin-left: -14rem;
            }
            .heading {
                font-size: 5rem;
            }
            .moto>p {
                font-size: 3rem;
            }
            .img1>img {
                width: 85rem;
                height: 65rem;
            }
            .pg-3 {
                margin-top: 23rem;
            }
            .footer {
                gap: 2rem;
            }
            .about>p {
                font-size: 3.5rem;
                padding-top: 2rem;
            }
            .icons {
                font-size: 3.5rem;
            }
            .add>p {
                font-size: 3.5rem;
            }
            .text-color>a {
                font-size: 3rem;
            }
        }
        
        .logo>img {
            max-width: 20rem;
        }
        
        .nav_ser>ul {
            align-items: flex-start;
            align-content: center;
            justify-content: center;
            padding-left: 4rem;
        }
    </style>
</head>

