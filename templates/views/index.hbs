                padding-top: 2rem;
            }
            .icons {
                font-size: 2.5rem;
            }
            .add>p {
                font-size: 3rem;
            }
            .text-color>a {
                font-size: 2.3rem;
            }
        }
        
        @media (min-width: 1000px) and (max-width: 1199px) {
            html {
                font-size: 32%;
            }
            .bod {
                height: 105rem;
            }
            .vid {
                width: 104rem;
                margin-left: 1rem;
            }
            .pg2 {
                height: fit-content;
                margin-top: 15rem;
                margin-bottom: 15rem;
            }
            .contentpg2 {
                justify-content: center;
            }
            .about>p {
                font-size: 3.5rem;
                padding-top: 2rem;
            }
            .icons {
                font-size: 3.5rem;
            }
            .add>p {
                font-size: 3.5rem;
            }
            .text-color>a {
                font-size: 3rem;
            }
        }
        
        @media (min-width: 800px) and (max-width: 999px) {
            html {
                font-size: 32%;
            }
            .bod {
                height: 105rem;
            }
            .vid {
                width: 92rem;
                margin-left: -6rem;
            }
            .footer {
                gap: 5rem;
            }
