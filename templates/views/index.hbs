    <style>
        * {
            margin: 0%;
            padding: 0%;
            box-sizing: border-box;
        }
        
        .height_anim {
            animation: height_anim 2s;
        }
        
        @keyframes height_anim {
            0% {
                height: 0%;
            }
            100% {
                height: fit-content;
            }
        }
        
        .hide {
            display: none;
        }
        
        .nav_ser>ul {
            overflow: hidden;
            transition: height 2s ease 0s;
        }
        
        html {
            font-size: 62.5%;
        }
        
        .Nav_main {
            position: absolute;
            display: flex;
            /* background-color: #9d9a9a; */
            flex-direction: row;
            align-items: center;
            justify-content: space-around;
            height: 20rem;
            width: 100vw;
            z-index: 11;
        }
        
        .Nav_main>.logo {
            position: relative;
            left: -16%;
        }
        
        .Nav_main>ul {
            display: flex;
            justify-content: space-between;
            align-items: center;
            gap: 2rem;
            list-style: none;
            height: 100%;
        }
        
        .Nav_main>ul>li {
            height: 100%;
            text-align: center;
            align-items: center;
            display: flex;
        }
        
        .ul_Uline {
            border-bottom: 4px solid;
        }
        
        .Nav_main>ul>li>a {
            color: white;
            text-decoration: none;
            font-size: 2rem;
        }
        
        .Nav_SS {
            font-size: 2rem;
            position: relative;
            left: 7%;
        }
        
        .Nav_main>div>button {
            font-size: 2.4rem;
        }
        
        .Nav_main>div>span {
            font-size: 2.5rem;
            cursor: default;
        }
    </style>
    <style>
        @media (min-width: 1500px) {
            html {
                font-size: 52%;
            }
            .vid {
                width: 78rem;
            }
            .contentpg2 {
                justify-content: flex-start;
                top: 13%;
                height: fit-content;
            }
        }
        
        @media (min-width: 1200px) and (max-width: 1499px) {
            html {
                font-size: 42%;
            }
            .bod {
                height: 116rem;
            }
            .vid {
                width: 88rem;
                margin-left: 11rem;
            }
            .pg2 {
                height: fit-content;
                margin-bottom: 15rem;
            }
